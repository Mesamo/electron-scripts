{
  "$schema": "./lib/configs/schema.json",
  "compilation": {
    "pkgJson": "template/package.json",
    "icon": "template/assets/icon.ico",
    "asar": {
      "unpack": "**/*/*config.json"
    },
    "product": {
      "appId": "{{DCF4678E-5487-4E63-B26F-8989175211F5}",
      "name": "MyApp",
      "version": "1.0.0",
      "dirName": "My App",
      "publisher": "Mesamo",
      "copyright": "Copyright Â© 1997-2005 Gsafety, Inc",
      "arch": "x64"
    },
    "mainProcess": {
      "main": "template/src/main.ts",
      "preload": "template/src/preload/preload.ts",
      "tsConfig": "template/tsconfig.app.json",
      "tslint": "template/tslint.json"
    },
    "rendererProcess": {
      "projects": [
        {
          "input": "template/renderer",
          "output": "."
        }
      ]
    }
  },
  "download": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  }
}
