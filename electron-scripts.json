{
  "$schema": "./lib/configs/schema.json",
  "compilation": {
    "pkgJson": "template/package.json",
    "icon": "template/assets/icon.ico",
    "asar": {
      "unpack": "**/*/*config.json"
    },
    "mainProcess": {
      "main": "template/src/main.ts",
      "tsConfig": "template/tsconfig.app.json",
      "tslint": "template/tslint.json"
    },
    "rendererProcess": {
      "projects": [
        {
          "input": "template/renderer",
          "output": "."
        }
      ]
    }
  },
  "download": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  }
}
