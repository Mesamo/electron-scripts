{
  "$schema": "./lib/configs/schema.json",
  "compilation": {
    "asar": {
      "unpack": "**/*/*config.json"
    },
    "mainProcess": {
      "main": "template/main.ts",
      "tsConfig": "tsconfig.app.json"
    },
    "rendererProcess": {
      "projects": [
        {
          "input": "renderer",
          "output": "."
        }
      ]
    }
  },
  "download": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  }
}
