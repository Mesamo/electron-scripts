{
  "$schema": "./node_modules/@mesamo/electron-scripts/lib/configs/schema.json",
  "compilation": {
    "pkgJson": "package.json",
    "icon": "assets/app-icon.ico",
    "asar": {
      "unpack": "**/*/*config.json"
    },
    "product": {
      "appId": "{{DCF4678E-5487-4E63-B26F-8989175211F5}",
      "dirName": "My App",
      "publisher": "Mesamo",
      "copyright": "Copyright Â© 1997-2005 Gsafety, Inc",
      "arch": "x64",
      "privileges": "admin"
    },
    "mainProcess": {
      "main": "src/main.ts",
      "preload": "src/preload/preload.ts",
      "tsConfig": "tsconfig.app.json",
      "tslint": "tslint.json"
    },
    "rendererProcess": {
      "projects": [
        {
          "input": "renderer",
          "output": "."
        }
      ]
    }
  },
  "download": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  }
}
